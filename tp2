#include <iostream>
#include <fstream>
#include <string>
#include <sstream>
#include <vector>

using namespace std;

int main()
{
	string mot;
	vector <string> dictionnaire;
	vector <string> compressed;
	int compteur = 0;
	fstream file;
	file.open("input.txt");
	file >> mot;
	dictionnaire.push_back(mot);
	compressed.push_back(mot);
	while (file >> mot)
	{
		cout << mot << endl;
		for (int i = 0; i < compteur; i++)
		{
			if (mot == dictionnaire[i])
			{
				dictionnaire[i].erase();
				dictionnaire.insert(dictionnaire.begin(), mot);
				compressed.push_back(to_string(i));
			}

		}
		if (dictionnaire[0] != mot)
		{
			dictionnaire.push_back(mot);
			compressed.push_back(mot);
		}

		compteur++;
	}

	for (auto i : compressed)
	{
		cout << i << " ";

	}
		
	

	return 0;
}
